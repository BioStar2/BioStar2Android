                        android:maxLines="2"
                        android:textColor="@color/content_text"
                        android:textSize="14dp"
                        androidfont:typeface="roboto_medium.ttf"/>
                </LinearLayout>

                <!-- block -->

                <FrameLayout
                    android:layout_width="match_parent"
                    android:layout_height="18dp"
                    android:background="@color/list_devider"/>
                <!--  -->
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="68dp"
                    android:layout_marginLeft="18dp"
                    android:layout_marginRight="18dp"
                    android:gravity="left"
                    android:orientation="horizontal">

                    <com.supremainc.biostar2.widget.StyledTextView
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_marginRight="18dp"
                        android:ellipsize="end"
                        android:gravity="left|center"
                        android:maxLines="2"
                        android:text="@string/status"
                        android:textColor="@color/select_text"
                        android:textSize="16dp"
                        androidfont:typeface="roboto_bold.ttf"/>


                    <com.supremainc.biostar2.widget.StyledTextView
                        android:id="@+id/status_name"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:ellipsize="end"
                        android:gravity="right|center"
                        android:maxLines="2"
                        android:textColor="@color/content_text"
                        android:textSize="14dp"
                        androidfont:typeface="roboto_medium.ttf"/>
                </LinearLayout>

                <FrameLayout
                    android:layout_width="match_parent"
                    android:layout_height="0.8dp"
                    android:background="@color/list_devider"/>
                <!--  -->

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="68dp"
                    android:layout_marginLeft="18dp"
                    android:layout_marginRight="18dp"
                    android:gravity="left"
                    android:orientation="horizontal">

                    <com.supremainc.biostar2.widget.StyledTextView
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_marginRight="18dp"
                        android:ellipsize="end"
                        android:gravity="left|center"
                        android:maxLines="2"
                        android:text="@string/period"
                        android:textColor="@color/select_text"
                        android:textSize="16dp"
                        androidfont:typeface="roboto_bold.ttf"/>

                    <com.supremainc.biostar2.widget.StyledTextView
                        android:id="@+id/period"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:ellipsize="end"
                        android:gravity="right|center"
                        android:maxLines="2"
                        android:textColor="@color/content_text"
                        android:textSize="14dp"
                        androidfont:typeface="roboto_medium.ttf"/>
                </LinearLayout>

                <FrameLayout
                    android:layout_width="match_parent"
                    android:layout_height="0.8dp"
                    android:background="@color/list_devider"/>
                <!--  -->

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="68dp"
                    android:layout_marginLeft="18dp"
                    android:layout_marginRight="18dp"
                    android:gravity="left"
                    android:orientation="horizontal">

                    <com.supremainc.biostar2.widget.StyledTextView
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_marginRight="18dp"
                        android:ellipsize="end"
                        android:gravity="left|center"
                        android:maxLines="2"
                        android:text="@string/access_group"
                        android:textColor="@color/select_text"
                        android:textSize="16dp"
                        androidfont:typeface="roboto_bold.ttf"/>

                    <com.supremainc.biostar2.widget.StyledTextView
                        android:id="@+id/access_group"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:ellipsize="end"
                        android:gravity="right|center"
                        android:maxLines="2"
