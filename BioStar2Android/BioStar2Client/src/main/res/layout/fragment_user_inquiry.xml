                                android:layout_width="105.8dp"
                                android:layout_height="105.8dp"
                                android:layout_gravity="center"
                                android:contentDescription="@string/image"
                                android:gravity="center"
                                android:scaleType="fitXY"
                                android:src="@drawable/user_photo_bg"/>
                        </FrameLayout>
                    </LinearLayout>
                </FrameLayout>

                <!--  -->

                <LinearLayout
