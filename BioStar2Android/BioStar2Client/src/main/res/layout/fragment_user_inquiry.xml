                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="9dp"
                                android:layout_marginRight="12dp"
                                android:ellipsize="end"
                                android:gravity="left|center"
                                android:singleLine="true"
                                android:text="@string/name"
                                android:textColor="@color/white"
                                android:textSize="18dp"
                                androidfont:typeface="roboto_bold.ttf"/>

                            <com.supremainc.biostar2.widget.StyledTextView
                                android:id="@+id/title_user_id"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="9dp"
                                android:layout_marginRight="12dp"
                                android:ellipsize="end"
                                android:gravity="left|center"
                                android:maxLines="2"
                                android:text="@string/user_id"
                                android:textColor="@color/white"
                                android:textSize="14dp"
                                androidfont:typeface="roboto_regular.ttf"/>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal">

                                <com.supremainc.biostar2.widget.StyledTextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginRight="4.5dp"
                                    android:ellipsize="end"
                                    android:gravity="left|center"
                                    android:text="@string/fingerprint"
                                    android:textColor="@color/transparent50_white"
                                    android:textSize="12dp"
                                    androidfont:typeface="roboto_regular.ttf"/>

                                <com.supremainc.biostar2.widget.StyledTextView
                                    android:id="@+id/title_finger_count"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginRight="9dp"
                                    android:ellipsize="end"
                                    android:gravity="left|center"
                                    android:text="0"
                                    android:textColor="@color/white"
                                    android:textSize="12dp"
                                    androidfont:typeface="roboto_regular.ttf"/>

                                <FrameLayout
                                    android:layout_width="0.8dp"
                                    android:layout_height="13.5dp"
                                    android:layout_marginRight="9dp"
                                    android:background="@color/transparent50_white"/>

                                <com.supremainc.biostar2.widget.StyledTextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginRight="4.5dp"
                                    android:ellipsize="end"
                                    android:gravity="left|center"
                                    android:text="@string/card"
                                    android:textColor="@color/transparent50_white"
                                    android:textSize="12dp"
                                    androidfont:typeface="roboto_regular.ttf"/>

                                <com.supremainc.biostar2.widget.StyledTextView
                                    android:id="@+id/title_card_count"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginRight="9dp"
                                    android:ellipsize="end"
                                    android:gravity="left|center"
                                    android:text="0"
                                    android:textColor="@color/white"
                                    android:textSize="12dp"
                                    androidfont:typeface="roboto_regular.ttf"/>

                                <FrameLayout
                                    android:id="@+id/title_pin_exist_devider"
                                    android:layout_width="0.8dp"
                                    android:layout_height="13.5dp"
                                    android:layout_marginRight="9dp"
                                    android:background="@color/transparent50_white"/>

                                <com.supremainc.biostar2.widget.StyledTextView
                                    android:id="@+id/title_pin_exist"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginRight="4.5dp"
                                    android:ellipsize="end"
                                    android:gravity="left|center"
                                    android:text="@string/pin_upper"
                                    android:textColor="@color/transparent50_white"
                                    android:textSize="12dp"
                                    androidfont:typeface="roboto_regular.ttf"/>
                            </LinearLayout>

                            <LinearLayout
